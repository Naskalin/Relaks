@page "/entries/{entryId:guid}"
@using Microsoft.AspNetCore.Mvc

@inject NavigationManager NavigationManager;

<Title>BaseEntry @EntryId</Title>

<p>EntryInfoId @EntryInfoId</p>
<p>NavigationManager @NavigationManager.Uri</p>

@code {
    [Parameter]
    public Guid EntryId { get; set; }
    
    public Guid? EntryInfoId { get; set; }

    protected override void OnInitialized()
    {
        var query = new Uri(NavigationManager.Uri).Query;
        var dict = System.Web.HttpUtility.ParseQueryString(query);
        var entryInfoId = dict["entryInfoId"];
        if (entryInfoId != null)
            EntryInfoId = Guid.Parse(entryInfoId);
            
    }

}